{
     "root": true,
     "parser": "@babel/eslint-parser",
     "parserOptions": {
          "requireConfigFile": false,
          "ecmaVersion": 12,
          "sourceType": "module"
     },
     "extends": [
          "eslint:recommended",
          "prettier"
     ],
     "env": {
          "es2021": true,
          "node": true
     },
     "rules": {
          "no-console": "warn"
     },
     "ignorePatterns": [
          "test/*"
     ]
}